{
  "name": "@feardread/security-agent",
  "version": "2.4.1",
  "description": "Advanced Security Testing and Analysis Framework with AI capabilities",
  "main": "index.js",
  "bin": {
    "security-agent": "./bin/cli.js",
    "fear-agent": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs": "jsdoc -c jsdoc.json",
    "build": "npm run lint && npm test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "security",
    "penetration-testing",
    "vulnerability-scanner",
    "code-analysis",
    "ai-security",
    "network-scanner",
    "cve-database",
    "security-audit",
    "proxy-manager",
    "traffic-monitor",
    "web-scraper",
    "api-tester",
    "security-testing",
    "threat-assessment"
  ],
  "author": "FearDread <ghaptonstall@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FearDread/fear-ai-agent.git"
  },
  "bugs": {
    "url": "https://github.com/FearDread/fear-ai-agent/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inquirer": "^8.2.5",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "anthropic": "^0.9.0",
    "openai": "^4.20.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!bin/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "files": [
    "index.js",
    "agent.js",
    "bin/",
    "controllers/",
    "routes/",
    "modules/",
    "libs/",
    "README.md",
    "LICENSE"
  ]
}
